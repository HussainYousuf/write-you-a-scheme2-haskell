comment ";" ;
token LispBool ({"#f"} | {"#t"}) ;
token LispNumber (('-')? digit);

Atom. LispVal ::= Ident;
String. LispVal ::= String;
Number. LispVal ::= LispNumber;
Bool. LispVal ::= LispBool;
Nil. LispVal ::= "Nil";
SExp. LispVal ::= "(" [LispVal] ")";
Quote. LispVal ::= "'" LispVal;
Prog. LispVal ::= [LispVal];

separator LispVal "";
